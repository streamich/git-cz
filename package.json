{
  "name": "git-cz",
  "version": "3.3.0",
  "description": "Semantic emojified git commit, git-cz",
  "main": "dist/cz.js",
  "bin": {
    "git-cz": "./bin/git-cz.js",
    "gitcz": "./bin/git-cz.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/streamich/git-cz.git"
  },
  "homepage": "https://github.com/streamich/git-cz",
  "license": "Unlicense",
  "scripts": {
    "lint": "yarn eslint",
    "clean": "rimraf dist binaries",
    "build": "yarn build:cli && yarn build:cz",
    "build:cli": "browserify --node -o dist/cli.js lib/cli.js",
    "build:cz": "browserify --node -o dist/cz.js --standalone prompter lib/cz.js",
    "build:readme": "mmarkdown",
    "build:binaries": "mkdirp binaries && pkg lib/cli.js --out-path binaries",
    "test": "mocha",
    "semantic-release": "semantic-release",
    "eslint": "eslint lib/*.js"
  },
  "devDependencies": {
    "semantic-release": "15.13.31",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "@semantic-release/npm": "5.3.4",
    "any-shell-escape": "0.1.1",
    "browserify": "16.5.0",
    "chalk": "2.4.2",
    "commitizen": "2.10.1",
    "eslint": "4.19.1",
    "eslint-config-mailonline": "9.0.0",
    "husky": "^3.1.0",
    "inquirer": "6.5.2",
    "minimist": "1.2.0",
    "pkg": "4.4.1",
    "rimraf": "3.0.0",
    "signale": "1.4.0",
    "word-wrap": "1.2.3",
    "mocha": "6.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "prepare-commit-msg": "exec < /dev/tty && node ./bin/git-cz.js --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./dist/cz.js"
    }
  },
  "mmarkdown": {
    "src": "./build/readme.md",
    "out": "./README.md",
    "scripts": "./build/readme.js",
    "backup": false
  },
  "dependencies": {
    "global": "^4.3.2"
  }
}
